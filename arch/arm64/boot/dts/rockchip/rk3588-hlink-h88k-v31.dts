	status = "okay";
};

&saradc {
	status = "okay";
	vref-supply = <&avcc_1v8_s0>;
};

&tsadc {
	status = "okay";
};

&rockchip_suspend {
	compatible = "rockchip,pm-rk3588";
	status = "okay";
	rockchip,sleep-debug-en = <1>;
	rockchip,sleep-mode-config = <
		(0
		| RKPM_SLP_ARMOFF_DDRPD
		)
	>;
	rockchip,wakeup-config = <
		(0
		| RKPM_GPIO_WKUP_EN
		| RKPM_USB_WKUP_EN
		)
	>;
};

&rng {
	status = "okay";
};

&sfc {
	status = "disabled";
	max-freq = <50000000>;
	#address-cells = <1>;
	#size-cells = <0>;
	pinctrl-names = "default";
	pinctrl-0 = <&fspim2_pins>;

	spi_flash: spi-flash@0 {
		#address-cells = <1>;
		#size-cells = <0>;
		compatible = "jedec,spi-nor";
		reg = <0x0>;
		spi-max-frequency = <50000000>;
		spi-tx-bus-width = <1>;
		spi-rx-bus-width = <4>;
		status = "okay";

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			loader@0 {
				label = "loader";
				reg = <0x0 0x1000000>;
			};
		};
	};
};

/* gmac0: eth2 */
&gmac0 {
	status = "okay";
	label = "eth2";
	pinctrl-names = "default";
	pinctrl-0 = <&gmac0_miim
		     &gmac0_tx_bus2
		     &gmac0_rx_bus2
		     &gmac0_rgmii_clk
		     &gmac0_rgmii_bus>;

	phy-handle = <&rgmii_phy>;

	clock_in_out = "output";
	phy-mode = "rgmii";
	tx_delay = <0x3e>;
	rx_delay = <0x2f>;

	snps,reset-gpio = <&gpio4 RK_PB3 GPIO_ACTIVE_LOW>;
	snps,reset-active-low;
	/* Reset time is 20ms, 100ms for rtl8211f */
	snps,reset-delays-us = <0 20000 100000>;
};

&mdio0 {
	rgmii_phy: phy@1 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <0x1>;
	};
};

/* Begin of USB
 * TYPEC0 
 *   usbdrd3_0 + usbdrd_dwc3_0 
 *   u2phy0 + u2phy0_otg + usbdp_phy0 + usbdp_phy0_u3
 */
&usbdrd3_0 {
	status = "okay";
};

&usbdrd_dwc3_0 {
	status = "okay";
	dr_mode = "otg";
	extcon = <&u2phy0>;
	usb-role-switch;

	port {
		#address-cells = <1>;
		#size-cells = <0>;
		dwc3_0_role_switch: endpoint@0 {
			reg = <0>;
			remote-endpoint = <&usbc0_role_sw>;
		};
	};
};

&u2phy0 {
	status = "okay";
};

&u2phy0_otg {
	status = "okay";
	vbus-supply = <&vbus5v0_typec>;
	rockchip,typec-vbus-det;
};

&usbdp_phy0 {
	status = "okay";
	orientation-switch;

	rockchip,dp-lane-mux = < 2 3 >;

	svid = <0xff01>;
	sbu1-dc-gpios = <&gpio0 RK_PC6 GPIO_ACTIVE_HIGH>;
	sbu2-dc-gpios = <&gpio0 RK_PD3 GPIO_ACTIVE_HIGH>;

	port {
		#address-cells = <1>;
		#size-cells = <0>;
		usbdp_phy0_orientation_switch: endpoint@0 {
			reg = <0>;
			remote-endpoint = <&usbc0_orien_sw>;
		};

		usbdp_phy0_dp_altmode_mux: endpoint@1 {
			reg = <1>;
			remote-endpoint = <&dp_altmode_mux>;
		};
	};
};

&usbdp_phy0_u3 {
	status = "okay";
};

&usbdp_phy0_dp {
	status = "okay";
};

/* TYPEC1
 * usbdrd3_1 + usbdrd_dwc3_1
 * u2phy1 + u2phy1_otg + usbdp_phy1 + usbdp_phy1_u3
 */
&usbdrd3_1 {
	status = "okay";
};

/*
 * H88K V3.1: usbdrd_dwc3_1 connect to Genesys GL3523 HUB
 *   HUB port 1 used for M.2 (B Key) socket for 4G/5G modem
 *   HUB port 2-4 used for USB Type-A port 
 */
&usbdrd_dwc3_1 {
	status = "okay";
};

&u2phy1 {
	status = "okay";
};

&u2phy1_otg {
	phy-supply = <&vcc5v0_usb_host>;
	status = "okay";
};

&usbdp_phy1 {
	status = "okay";
};

&usbdp_phy1_u3 {
	status = "okay";
};

&usbdp_phy1_dp {
	status = "okay";
};

/* USB20_HOST0
 * usb_host0_ehci + usb_host0_ohci
 * u2phy2 + u2phy2_host
 */ 
&usb_host0_ehci {
	status = "okay";
};

&usb_host0_ohci {
	status = "okay";
};

&u2phy2 {
	status = "okay";
};

&u2phy2_host {
	status = "okay";
};

/* USB20_HOST1
 * usb_host1_ehci + usb_host1_ohci
 * u2phy3 + u2phy3_host
 */
&usb_host1_ehci {
	status = "okay";
};

&usb_host1_ohci {
	status = "okay";
};

&u2phy3 {
	status = "okay";
};

&u2phy3_host {
	status = "okay";
};

/*
 * pce2x1l0 or sata1 => combphy1_ps
 * H88K v3.1: pcie2x1l0 is M.2 (A+E Key) socket for WiFi/Bluetooth
 */
&combphy1_ps {
	status = "okay";
};

&pcie2x1l0 {
	reset-gpios = <&gpio0 RK_PB0 GPIO_ACTIVE_HIGH>;
	status = "okay";
	vpcie3v3-supply = <&vcc3v3_sys>;
};

&sata1 {
	status = "disabled";
}; 

/*
 * pcie2x1l1 or sata2 or usb30_2 => combphy2_psu
 * combphy2_psu: pcie2x1l1 or sata2 or (usbhost3_0 + usbhost_dwc3_0)
 * H88K V3.1: pcie2x1l1 used for rtl8125b
 */
&combphy2_psu {
	status = "okay";
};

&usbhost3_0 {
	status = "disabled";
};

&usbhost_dwc3_0 {
	status = "disabled";
};

/* rtl8125b: eth1 */
&pcie2x1l1 {
	reset-gpios = <&gpio4 RK_PA2 GPIO_ACTIVE_HIGH>;
	status = "okay";
	vpcie3v3-supply = <&vcc3v3_pcie20>;

	pcie@0,0 {
		reg = <0x00300000 0 0 0 0>;
		#address-cells = <3>;
		#size-cells = <2>;

		rtl8125_1: pcie-eth@0,0 {
			compatible = "pci10ec,8125";
			label = "eth1";
			reg = <0x000000 0 0 0 0>;

			/*
			 * G+ 3v3
			 * G- LED3: ACT on all speed
			 * Y+ 3v3
			 * Y- LED2: LINK on 10 + 100 + 1000 + 2500
			 */
			r8125,led2 = <0x002b>;
			r8125,led3 = <0x0200>;
		};
	};
};

&sata2 {
	status = "disabled";
}; 

/*
 * pcie2x1l2 or sata0 => combphy0_ps
 * H88K V3.1 : pcie2x1l2 used for rtl8125b
 */
&combphy0_ps {
	status = "okay";
};

/* rtl8125b eth0 */
&pcie2x1l2 {
	reset-gpios = <&gpio4 RK_PA5 GPIO_ACTIVE_HIGH>;
	status = "okay";
	vpcie3v3-supply = <&vcc3v3_pcie20>;

	pcie@0,0 {
		reg = <0x00400000 0 0 0 0>;
		#address-cells = <3>;
		#size-cells = <2>;

		rtl8125_2: pcie-eth@0,0 {
			compatible = "pci10ec,8125";
			label = "eth0";
			reg = <0x000000 0 0 0 0>;

			/*
			 * G+ 3v3
			 * G- LED3: ACT on all speed
			 * Y+ 3v3
			 * Y- LED2: LINK on 10 + 100 + 1000 + 2500
			 */
			r8125,led2 = <0x002b>;
			r8125,led3 = <0x0200>;
		};
	};
};

&sata0 {
	status = "disabled";
};

/*
